/*
 * 显示并刷新oled
 */
void OLEDprint()
{
  u8g2.firstPage();
  do {
    u8g2.setCursor(0, 15);        //设置光标处
    u8g2.print(printString1);  
    u8g2.setCursor(0, 31);
    u8g2.print("Speed:");
    u8g2.setCursor(80, 31);
    u8g2.print(printString2);
    u8g2.setCursor(0, 47);
    u8g2.print("Vol_state:");
    u8g2.setCursor(80, 47);
    u8g2.print(printString3);
    u8g2.setCursor(0, 63);
    u8g2.print(printString4);
  } while ( u8g2.nextPage() );
}

/*
 * 连续 NumOfSample 次测距
 * x为调用的超声波传感器编号
 */
int getDis(int x)
{
  float distance; 
  digitalWrite(Trig[x], LOW); 
  delayMicroseconds(2); 
  digitalWrite(Trig[x], HIGH); 
  delayMicroseconds(10);
  digitalWrite(Trig[x], LOW);
  unsigned long dis_start_time = millis();
  distance = pulseIn(Echo[x], HIGH) * 0.017;
  if(distance > 100)
    return 0;
  if(pos < NumOfAdd)
  {
    dis_add += distance;
    pos++;
    return getDis(x);
  }
  return 1;  //测距正常完成
}

/*
 * 单超声波完整程序
 */
void Ultrasonic(int x)
{
  pos = 0;
  dis_add = 0;
  dis_flag = getDis(x);
  if(dis_flag)
  {
    dis_aver = dis_add/NumOfAdd;  //取平均
    /*
    Serial.print("Ultra");
    Serial.print(x);
    Serial.print(": ");
    Serial.println(dis_aver);
    */
  }
}

/*
 * 行进过程中检测是否需要刹车并采取行动
 */
void checkAndStop()
{
  for(int i = 0; i < wave_Num; i++)
  {
    if(moveState[i] == forbid[i])
    {
      Stop();
      moveState[i] = 0;
    }
  }
}

void press_key()
{
  menu++;
  if(menu > 2)
    menu = 1;
}

void Main()
{
  serialEvent8266();
  
  if(millis()-start_time > 500)  //给舵机0.5秒旋转时间
  {
    myservo.detach();  //释放定时器，避免与串口共用定时器导致的舵抖
  }
 
  if(print_flag)
  {
    for(int i = 0; i < wave_Num; i++)
    {
      Ultrasonic(i);
      if(dis_flag && ((abs(speed) > 0.1 && dis_aver < abs(speed/3)) || (abs(speed) < 0.1 && dis_aver < 20)))   //小于安全距离就停止向此方向运动
      {
        forbid[i] = 1;
        checkAndStop();
      }
      else
      {
        forbid[i] = 0;
      }
      printString4 += String(dis_aver) + "   ";
    }
    OLEDprint();  //调试可用，实际使用别开，每0.2s刷新一次oled太消耗cpu资源了
    printString4 = "";
    print_flag = 0;
  }
  
  if(delay_count < 300 && delay_count >= 0) //一分钟统计一次电量
    delay_count++;
  else
  {
    delay_count = 0;
    get_vol();
    //Serial.print("Vol ");
    //Serial.println(Cal_vol());
    printString3 = String(Cal_vol());
    OLEDprint();
    if(!Cal_vol())  //没电自动停车归位,保护电池
    {
      Stop();
      TURNMID;
    }
  }
}

void loop()
{
  switch(menu)
  {
    case 1:
    {
      Main();
      return;
    }
    case 2: //调试时可用作直接停车按钮
    {
      TURNMID;
      Stop();
      dcb();
      return;
    }
  }
}
