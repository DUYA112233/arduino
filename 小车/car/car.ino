/*
 * 控制步进电机
 */
void advance()
{ 
  digitalWrite(M,LOW);
  analogWrite(E,PWM);
}

void back()
{  
  digitalWrite(M,HIGH);
  analogWrite(E,PWM/24*25); //前进转的会慢一点，纠正误差
}

void Stop()
{
  digitalWrite(E, LOW); //电机停
}  

/*
 * 测速程序
 */
void begin_get_speed()
{
  attachInterrupt(0, Code,FALLING); // 重新开放外部中断0
}

/*
 * 统计上一个delay_time中的脉冲数并转换为每分钟转速输出
 */
float finish_get_speed()
{
  detachInterrupt(0); // 关闭外部中断0
  rpm =(float)count / (delay_time/1000.0) * 60.0 / (PerRound * Reduction_ratio);
//每分钟转过圈数=一个测速周期内测得count除以测速周期*60秒每分/（转一圈得到脉冲数*减速比）
  if(!forward_or_back) rpm = 0 - rpm;
  count = 0;   //把脉冲计数值清零，以便计算下一个delay_time的脉冲计数
  return rpm;  
}

/*
 * 超过限制范围时取限制范围上下限
 */
int ServoLimit(int input)
{
  if(input < 60) return 60;
  else if(input > 140) return 140;
  else return input;
}

int MotorLimit(int input)
{
  if(input < 60) return 60;  //60以下电机不会转
  else if(input > 200) return 200;
  else return input;
}

float SpeedLimit(float output)
{
  if(abs(output) < 35) return 0; //不可能出现35以下转速
  else if(output > 180) return 180; //最大不会超过180
  else if(output < -180) return -180;
  else return output;
}

/*
 * 将字符串中的数字转换为int
 */
int Cal()
{
  if(readString[3] >= '0' && readString[3] <= '9')
    return (readString[1]-48)*100 + (readString[2]-48)*10 + (readString[3]-48);
  else if(readString[2] >= '0' && readString[2] <= '9')
    return (readString[1]-48)*10 + (readString[2]-48);
  else
    return (readString[1]-48);
}

/*
 *  输入字符串，确定从第几位开始解析后面多少位
 *  input:需解析的字符串
 *  Matching:匹配样本
 *  num_start:从第几位开始解析（第一位为字符串开始）
 *  flag:输出1为匹配成功，0为失败
 */
bool AnalysisGetString(String input, String Matching, int num_start)
{
  bool flag = 1;
  int len = Matching.length();
  for(int i = 0; i < len; i++)
  {
    if(input[num_start+i-1] != Matching[i])
    {
      flag = 0;
      break;
    }
    else continue;
  }
  return flag;
}

/*
 * 测量电压函数
 */
void get_vol()
{
  get_num = analogRead(Vol_Pin);
  if(get_num > 890) return;  //大于4.35V认为异常
  if(get_num != last_num)
  {
    last_num = get_num;
    vol = get_num * 5.0 / 1023.0;
  }
}

/*
 * 通过电压给出电量情况
 * 分析实验数据得：
 * 电量  电压
 * 100%  4.07
 *  80%  3.89
 *  60%  3.70
 *  40%  3.54
 *  20%  3.39
 *   5%  3.19
 *   0%  3.00
 *   2.00 以下肯定是没接线
 */
int Cal_vol()
{
  if(vol < 2.00) return -1;
  if(vol >= 3.89) return 5;
  else if(vol >= 3.70 && vol < 3.89) return 4;
  else if(vol >= 3.54 && vol < 3.70) return 3;
  else if(vol >= 3.39 && vol < 3.54) return 2;
  else if(vol >= 3.19 && vol < 3.39) return 1;
  else return 0;
}

/*
 * client端发送信息后解析信息并让电机干活
 * 数据格式：
 *    控制舵机：      ps(数值范围60-140)
 *        L/l + 数值 + $ : 向左转数值度数（大约）
 *        R/r + 数值 + $ : 向右转数值度数（大约）
 *        M/m +        $ : 转回中间位置
 *    控制步进电机：  ps(数值范围60-200)
 *        A/a + 数值 + $ : 设定前进速度并前进
 *        B/b + 数值 + $ : 设定后退速度并后退
 *        S/s +        $ : 停止
 */
void serialEvent8266()
{
  if (EspSerial.available())
  {
    char c = EspSerial.read(); //读取一字节数据
    if (c == '$')
    {
      //Serial.println(readString);
      if(AnalysisGetString(readString, "IP:", 1))
      {
        printString4 = readString;
        OLEDprint();
      }
      else if((readString[0] == 'l') + (readString[0] == 'L'))
      {
        moveState[2] = 1;
        SERVOMIN = ServoLimit(110 - Cal());//将输入（要转过的角度）转化 (*5/3)
        //Serial.println("turn left");
        myservo.attach(SERVOPIN);
        TURNLEFT;
        start_time = millis();
      }
      else if ((readString[0] == 'r') + (readString[0] == 'R'))
      {
        moveState[3] = 1;
        SERVOMAX = ServoLimit(110 + Cal()); 
        //Serial.println("turn right");
        myservo.attach(SERVOPIN);
        TURNRIGHT;
        start_time = millis();
      } 
      else if ((readString[0] == 'm') + (readString[0] == 'M'))
      {
        moveState[2] = 0;
        moveState[3] = 0;
        //Serial.println("turn middle");
        myservo.attach(SERVOPIN);
        TURNMID;
        start_time = millis();
      } 
      else if (((readString[0] == 'a') + (readString[0] == 'A')) && !forbid[0]) //0是前超声波
      { 
        moveState[0] = 1;
        PWM = MotorLimit(Cal());
        Stop();           //小车先暂停
        delay(10);
        advance();      //小车再前进
        count = 0; //恢复小车编码器初始计数状态
        //Serial.println("go ahead");
      }
      else if (((readString[0] == 'b') + (readString[0] == 'B')) && !forbid[1])
      {
        moveState[1] = 1;
        PWM = MotorLimit(Cal());
        Stop();           //小车先暂停
        delay(10);
        back();      //小车再后退
        count = 0; //恢复小车编码器初始计数状态
        //Serial.println("go back");
      }
      else if ((readString[0] == 's') + (readString[0] == 'S'))
      {
        moveState[0] = 0;
        moveState[1] = 0;
        Stop();        //小车停止 
        //Serial.println("Stop");
      }
      else if (readString[0] == 'c')
      {
        readString = "Connected";
        count = 0;
        send_or_not = 1;
        begin_get_speed();
        wdt_enable(WDTO_1S); //1s 看门狗
        FlexiTimer2::start();
      }
      else if (readString[0] == 'd')
      {
        readString = "disConnected";
        wdt_disable();  //停止看门
        send_or_not = 0;  //客户端断连时停止发送，防止堵塞
        FlexiTimer2::stop();
        OledSetUp();
      }
      printString1 = readString;
      readString = "";
      OLEDprint();
      if(send_or_not == 0)//如果要显示的时断开连接，显示后暂停1s，
        delay(1000);      //不然会直接回到初始界面看不到这条信息
    }
    else
    {
      for(int i = 0; i < allowChar_Num; i++)
      {
        if(c == allowChar[i])
        {
          readString += c; //拼接数据
        }
      }
    }
  }
}

