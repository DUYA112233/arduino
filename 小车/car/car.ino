{
  wdt_reset();
  if(send_or_not)
  {
    speed = SpeedLimit(finish_get_speed());
    printString2 = String(speed);
    EspSerial.print("SP");
    EspSerial.println(speed);
    EspSerial.print('$');
    print_flag = 1; //提示主程序该刷新oled了
    begin_get_speed();
  }
}

